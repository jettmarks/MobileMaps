<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
 <display-name>Bike Trains</display-name>
  
 <servlet>
  <servlet-name>getTags</servlet-name>
  <display-name>Get Tags</display-name>
  <servlet-class>com.jettmarks.routes.server.GetTagsImpl</servlet-class>
 </servlet> 
 
 <servlet>
  <servlet-name>getDisplayElements</servlet-name>
  <servlet-class>com.jettmarks.routes.server.GetDisplayElementsImpl</servlet-class>
 </servlet>
 
 <servlet-mapping>
  <servlet-name>getTags</servlet-name>
  <url-pattern>/getTags</url-pattern>
 </servlet-mapping>
 
 <servlet-mapping>
  <servlet-name>getDisplayElements</servlet-name>
  <url-pattern>/getDisplayElements</url-pattern>
 </servlet-mapping>
 
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>
